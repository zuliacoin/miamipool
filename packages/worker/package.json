{
  "name": "worker-typescript-template",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "dist/worker.js",
  "scripts": {
    "build": "esbuild --bundle --sourcemap --outdir=dist ./src/index.js",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}'",
    "dev": "miniflare ./dist/index.js --watch --debug",
    "deploy": "wrangler publish",
    "test": "yarn build && ava --verbose test/*.spec.js"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "type": "module",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@types/jest": "^26.0.23",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "ava": "^3.15.0",
    "esbuild": "^0.13.2",
    "jest": "^27.0.1",
    "miniflare": "^1.4.1",
    "prettier": "^2.3.0",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "itty-router": "^2.4.4",
    "micro-stacks": "^0.0.30",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.0"
  },
  "browser": {
    "crypto": false
  }
}
